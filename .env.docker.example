# Docker Environment Configuration
# Copy this to .env when using Docker Compose
# This file is used by docker-compose.yml

# Application Port
PORT=3030

# PostgreSQL Configuration
POSTGRES_DB=minesweeper
POSTGRES_USER=minesweeper_user
POSTGRES_PASSWORD=change-this-secure-password-in-production
POSTGRES_PORT=5432

# Daily Puzzle Seed Salt
DAILY_SEED_SALT=minesweeper-daily-puzzle-salt-2025

# SSL (false for Docker internal network, true for production)
POSTGRES_SSL=false
